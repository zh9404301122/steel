<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.steel.mapper.LoginMapper">

    <select id="queryLoginVo" parameterType="java.lang.String" resultType="com.example.steel.vo.LoginVo">
        select id,userName,password,vserName,phone from t_sys_user where userName = #{userName}
